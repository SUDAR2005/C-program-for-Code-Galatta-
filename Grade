#include <stdio.h>
int main(){
int num;
printf("Enter your mark ");
	if(num >= 80){
	printf(" You got A grade");
		}
	else if ( num >=60){ // Note the space between else & if
		printf(" You got B grade");
		}
	else if ( num >=40){
		printf(" You got C grade");
		}
	else if ( num < 40){
		printf(" You Failed in this exam");
		}
return 0;
}
