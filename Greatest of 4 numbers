include <stdio.h>
 
int main()
{
    int a, b, c,d;
    printf("\nEnter four numbers:\t");
    scanf("%d%d%d%d", &a,&b,&c,&d);
    if (a > b && a > c && a>d)
        printf("Biggest number is %d", a);
    else  if (b>c && b > d)
        printf("Biggest number is %d", b);
    else if (c > d)
        printf("Biggest number is %d", c);
    else
        printf("Biggest number is %d",d);
    return 0;
}
